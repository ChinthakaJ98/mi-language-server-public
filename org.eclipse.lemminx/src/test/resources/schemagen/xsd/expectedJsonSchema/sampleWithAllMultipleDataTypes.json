{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "SampleData",
    "type": "object",
    "properties": {
        "StringValue": {
            "type": "string"
        },
        "DecimalValue": {
            "type": "number"
        },
        "IntegerValue": {
            "type": "integer"
        },
        "BooleanValue": {
            "type": "boolean"
        },
        "DateValue": {
            "type": "string",
            "format": "date"
        },
        "TimeValue": {
            "type": "string",
            "format": "time"
        }
    },
    "required": ["StringValue", "DecimalValue", "IntegerValue", "BooleanValue", "DateValue", "TimeValue"]
}
