{
    "$schema": "http://wso2.org/json-schema/wso2-data-mapper-v5.0.0/schema#",
    "id": "http://wso2jsonschema.org",
    "type": "object",
    "properties": {
        "Name": {
            "id": "http://wso2jsonschema.org/Name",
            "type": "string"
        },
        "Age": {
            "id": "http://wso2jsonschema.org/Age",
            "type": "number"
        },
        "IsMember": {
            "id": "http://wso2jsonschema.org/IsMember",
            "type": "boolean"
        },
        "FavoriteColor": {
            "id": "http://wso2jsonschema.org/FavoriteColor",
            "type": "string",
            "enum": [
                "Red",
                "Green",
                "Blue"
            ]
        }
    },
    "required": [
        "Name",
        "Age",
        "IsMember",
        "FavoriteColor"
    ],
    "title": "Person"
}
